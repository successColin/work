.news_page{
	/* margin-bottom: 2rem; */
	.backbar {
	    border-bottom: solid 1px transparent !important;
	}
	.news_header{
		position: fixed;
		top: 0;
		height: 8rem;
		z-index: 1000;
		width: 100%;
		.news_search{
			margin-top: 1.5rem;
		}
	}
	.getMore{
		    width: pxToREM(160);
		    height: pxToREM(60);
			line-height: pxToREM(56);
		    background: white;
		    border-radius: pxToREM(30);
		    border: 2px solid #c62127;
		    font-size: pxToREM(30);
		    color: #c62127;
		    text-align: center;
		    margin: 0 auto;
	}
	.showNoData{
		color: #999;
		text-align: center;
		font-size: .65rem;
		margin-top: 1rem;
	}
}
.news_container{
	background-color: #fff;
	margin-top: .7rem;
	display: flex;
	flex-direction: column;
	padding: .9rem;
	/* margin-bottom: 2rem; */
	.news_item{
		position: relative;
		.news_title{
			position: absolute;
			top: 0;
		}
		.news_tips2{
			position: absolute;
			bottom: .2rem;
		}
	}
	.news_title{
		color: #444444;
		font-size: 1rem;
	}
	.news_img{
		display: flex;
		flex-direction: row;
		justify-content: flex-start;
		margin-top: pxToREM(10);
		.mgl{
			margin-left: .7rem;
		}
		}
		.news_img img{
			width: pxToREM(180);
			height: pxToREM(122);
			border-radius: pxToREM(6);
		}
		.news_tips{
			font-size: pxToREM(22);
			color: #9B9B9B;
			margin-top: 10px;
			width: 100%;
			border-bottom: 1px solid #EBEBEB;
			padding-bottom: 5px;
		}
		.news_wrap{
			margin-bottom: 10px;
		}
		.news_layout2{
			display: flex;
			flex-direction: row;
			align-items: center;
			border-bottom: 1px solid #EBEBEB;
			padding-bottom: 5px;
			.news_tips2{
				font-size: .75rem;
				color: #9B9B9B;
				margin-top: 10px;
				width: 100%;
			}
			.news_layout2_left{
				flex: 4;
				padding-right: 1rem;
				display: flex;
				flex-direction: column;
			}
			.news_layout2_right{
				flex: 1;
			}
			
		}
		.news_layout2_img{
			justify-content: end;
			align-self: center;
		}
}
